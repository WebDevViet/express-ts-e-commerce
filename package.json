{
  "name": "node-js-typescript",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "dev": "bun --watch run ./src/app.ts",
    "build": "rimraf ./dist && bun run build.js",
    "build-rollup": "rimraf ./dist && rollup -c",
    "start": "bun dist/app.js",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "prettier": "prettier --check .",
    "prettier-fix": "prettier --write .",
    "check-format": "bun lint && bun prettier",
    "fix-format": "bun lint-fix && bun prettier-fix",
    "prepare": "husky",
    "commitlint": "commitlint --edit"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "bun run fix-format",
      "git add ."
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "^9.18.0",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/bcrypt": "^5.0.2",
    "@types/bun": "latest",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/debug": "^4.1.12",
    "@types/express": "^5.0.0",
    "@types/formidable": "^3.4.5",
    "@types/http-errors": "^2.0.4",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/ms": "^2.1.0",
    "@types/omit-empty": "^1.0.4",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "i": "^0.3.7",
    "lint-staged": "^15.5.1",
    "npm": "^11.3.0",
    "rollup": "^4.40.0",
    "typescript-eslint": "^8.20.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "chalk": "^5.4.1",
    "change-case": "^5.4.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "generate-password": "^1.7.1",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.15.0",
    "morgan": "^1.10.0",
    "ms": "^2.1.3",
    "nanoid": "^5.1.5",
    "omit-empty": "^1.0.0",
    "prettier": "^3.4.2",
    "radashi": "^12.3.4",
    "rimraf": "^6.0.1",
    "tsc-alias": "^1.8.10",
    "zod": "^3.24.1"
  }
}
